@using PagedList.Mvc;
@model PagedList.IPagedList<Invest.Core.Category>

@{
    ViewBag.Title = "Index";
}

@Html.Partial("Breadcrumb", new Dictionary<string, string> { { "Category", Url.Action("", "Category", new { area = "Admin" }, null) } })


<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well">
            <h2>Manage Categories</h2>
        </div>
        <div class="box-content">
            @Html.Partial("_PartialCategory", Model)
        </div>
    </div>
</div>


